package com.xmlphotoalbum.Mp3Buttons{	import flash.display.Sprite;	import flash.display.CapsStyle;	import flash.display.JointStyle;	import flash.display.LineScaleMode;		public class trackerBarButton extends Sprite {		public var Player:Sprite;		public var color:uint;		public var TrBarWidth:Number;		public function trackerBarButton(_color:uint,_TrBarWidth:Number):void {						color = _color;			TrBarWidth = _TrBarWidth;			Player = new Sprite();			addChild(Player);			var trackerBar:Sprite = new Sprite();			trackerBar.graphics.lineStyle(1,color,1, true, LineScaleMode.NORMAL, CapsStyle.SQUARE, JointStyle.MITER);			trackerBar.graphics.moveTo(10,26);			trackerBar.graphics.lineTo(TrBarWidth,26);						Player.addChild(trackerBar);					}			}	}