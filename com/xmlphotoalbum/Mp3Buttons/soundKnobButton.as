package com.xmlphotoalbum.Mp3Buttons{	import flash.display.Sprite;	import flash.display.*;	import flash.events.*;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;	import flash.text.AntiAliasType;	import caurina.transitions.*;	public class soundKnobButton extends Sprite {		public var Player:Sprite;		public function soundKnobButton():void {			Player = new Sprite();			addChild(Player);			var soundKnob:Sprite = new Sprite();			soundKnob.graphics.beginFill(0xffffff);			soundKnob.graphics.drawCircle(2, 10, 3);			soundKnob.graphics.endFill();			soundKnob.x = 117;						Player.addChild(soundKnob);			soundKnob.buttonMode 	   = true;									var alphaVal:Number = 0.5; 						soundKnob.alpha = alphaVal;						soundKnob.addEventListener ('mouseOver', function() {				Tweener.addTween(soundKnob, {alpha:1, time:1});			});						soundKnob.addEventListener ('mouseOut', function() {				Tweener.addTween(soundKnob, {alpha:alphaVal, time:1});			});					}			}	}