package com.xmlphotoalbum.Mp3Buttons{	import flash.display.Sprite;	import flash.display.*;	import flash.events.*;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;	import flash.text.AntiAliasType;	import caurina.transitions.*;	public class NextButton extends Sprite {		public var Player:Sprite;		public function NextButton():void {			Player = new Sprite();			addChild(Player);			var nextBtn:Sprite = new Sprite();			nextBtn.graphics.beginFill(0xffffff);			nextBtn.graphics.lineStyle(0, 0xffffff,0, true, LineScaleMode.NORMAL, CapsStyle.SQUARE, JointStyle.MITER);			nextBtn.graphics.moveTo(19,6);			nextBtn.graphics.lineTo(15,8);			nextBtn.graphics.lineTo(15,6);			nextBtn.graphics.lineTo(9,9);			nextBtn.graphics.lineTo(15,13);			nextBtn.graphics.lineTo(15,11);			nextBtn.graphics.lineTo(19,13);			nextBtn.graphics.endFill();			nextBtn.scaleX = -1;			nextBtn.x = 85;						Player.addChild(nextBtn);			nextBtn.buttonMode 	   = true;									var alphaVal:Number = 0.5; 						nextBtn.alpha = alphaVal;						nextBtn.addEventListener ('mouseOver', function() {				Tweener.addTween(nextBtn, {alpha:1, time:1});			});						nextBtn.addEventListener ('mouseOut', function() {				Tweener.addTween(nextBtn, {alpha:alphaVal, time:1});			});					}			}	}