package com.xmlphotoalbum.Mp3Buttons{	import flash.display.Sprite;	import flash.display.*;	import flash.events.*;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;	import flash.text.AntiAliasType;	import caurina.transitions.*;	public class PreviousButton extends Sprite {		public var Player:Sprite;		public function PreviousButton():void {			Player = new Sprite();			addChild(Player);			var previousBtn:Sprite = new Sprite();			previousBtn.graphics.beginFill(0xffffff);			previousBtn.graphics.lineStyle(0, 0xffffff,0, true, LineScaleMode.NORMAL, CapsStyle.SQUARE, JointStyle.MITER);			previousBtn.graphics.moveTo(19,6);			previousBtn.graphics.lineTo(15,8);			previousBtn.graphics.lineTo(15,6);			previousBtn.graphics.lineTo(9,9);			previousBtn.graphics.lineTo(15,13);			previousBtn.graphics.lineTo(15,11);			previousBtn.graphics.lineTo(19,13);			previousBtn.graphics.endFill();						Player.addChild(previousBtn);			previousBtn.buttonMode 	   = true;									var alphaVal:Number = 0.5; 						previousBtn.alpha = alphaVal;						previousBtn.addEventListener ('mouseOver', function() {				Tweener.addTween(previousBtn, {alpha:1, time:1});			});						previousBtn.addEventListener ('mouseOut', function() {				Tweener.addTween(previousBtn, {alpha:alphaVal, time:1});			});					}			}	}