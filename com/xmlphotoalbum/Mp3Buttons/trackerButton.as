package com.xmlphotoalbum.Mp3Buttons{	import flash.display.Sprite;	import flash.display.*;	import flash.events.*;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;	import flash.text.AntiAliasType;	import caurina.transitions.*;	public class trackerButton extends Sprite {		public var Player:Sprite;		public function trackerButton():void {			Player = new Sprite();			addChild(Player);			var tracker:Sprite = new Sprite();			tracker.graphics.beginFill(0xffffff);			tracker.graphics.drawCircle(12, 26, 3);			tracker.graphics.endFill();									Player.addChild(tracker);			tracker.buttonMode 	   = true;									var alphaVal:Number = 0.5; 						tracker.alpha = alphaVal;						tracker.addEventListener ('mouseOver', function() {				Tweener.addTween(tracker, {alpha:1, time:1});			});						tracker.addEventListener ('mouseOut', function() {				Tweener.addTween(tracker, {alpha:alphaVal, time:1});			});					}			}	}